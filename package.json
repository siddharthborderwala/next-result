{
  "name": "next-result",
  "version": "0.1.0-beta.1",
  "description": "A result type for Next.js, inspired by rust's Result type.",
  "source": "src/index.ts",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "unpkg": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "nextjs",
    "result",
    "error",
    "success",
    "typescript",
    "rust"
  ],
  "author": "Siddharth Borderwala",
  "license": "MIT",
  "devDependencies": {
    "typescript": "latest",
    "tsup": "8.4.0"
  }
}
